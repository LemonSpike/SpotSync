target 'SpotSyncApp' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SpotSyncApp

  pod 'Alamofire', '~> 4.4'
  pod 'AlamofireObjectMapper', '~>4.1'
  pod 'AlamofireImage', '~> 3.1'

  target 'SpotSyncAppTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SpotSyncAppUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
